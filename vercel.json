{
  "version": 2,
  "routes": [
    { "src": "/logo.png", "dest": "/public/logo.png" },
    { "src": "/api/(.*)", "dest": "/server/server.js" },
    { "src": "/css/(.*)", "dest": "/client/css/$1" },
    { "src": "/js/(.*)", "dest": "/client/js/$1" },
    { "src": "/seller.html", "dest": "/client/seller.html" },
    { "src": "/login.html", "dest": "/client/login.html" },
    { "src": "/(.*)", "dest": "/client/index.html" }
  ]
}
